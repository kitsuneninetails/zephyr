{
  "log_dir": "/tmp/zephyr/logs",
  "overlay": {
    "class": "zephyr.midonet.midonet_overlay.MidonetOverlay",
    "args": {
      "midonet_api_url": "http://localhost:8181/midonet-api"
    }
  },
  "hosts": {
    "master-ds": {
      "host_type": "local",
      "vm_type": "zephyr.vtm.underlay.ipnetns_vm.IPNetnsVM",
      "hypervisor": true
    }
  }
}
